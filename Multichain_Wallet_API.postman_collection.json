{
  "info": {
    "name": "Multichain Wallet API",
    "description": "Complete API collection for the Multichain Wallet supporting Bitcoin, Ethereum, Solana, and 20+ other blockchains with comprehensive token registry",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "apikey",
    "apikey": [
      {
        "key": "value",
        "value": "{{api_key}}",
        "type": "string"
      },
      {
        "key": "key",
        "value": "X-API-KEY",
        "type": "string"
      }
    ]
  },
  "item": [
    {
      "name": "Health & Info",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/health",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "health"]
            },
            "description": "Check if the API is running and healthy"
          }
        },
        {
          "name": "Get Supported Wallet Types",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/wallet/types",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "wallet", "types"]
            },
            "description": "Get all supported blockchain wallet types including Bitcoin variants, EVM chains, Cosmos ecosystem, etc."
          }
        },
        {
          "name": "Get Supported Languages",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/mnemonic/languages",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "mnemonic", "languages"]
            },
            "description": "Get all supported mnemonic languages (English, Japanese, Korean, Spanish, Chinese, French, Italian, Czech, Portuguese)"
          }
        }
      ]
    },
    {
      "name": "Mnemonic Management",
      "item": [
        {
          "name": "Generate Mnemonic",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"word_count\": 12,\n  \"language\": \"english\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/mnemonic/generate",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "mnemonic", "generate"]
            },
            "description": "Generate a new BIP39 mnemonic phrase. Word count can be 12, 15, 18, 21, or 24"
          }
        },
        {
          "name": "Validate Mnemonic",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"mnemonic\": \"abandon abandon abandon abandon abandon abandon abandon abandon abandon abandon abandon about\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/mnemonic/validate",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "mnemonic", "validate"]
            },
            "description": "Validate a BIP39 mnemonic phrase"
          }
        }
      ]
    },
